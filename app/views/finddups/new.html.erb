<h1 align = 'center'> Conseguir duplicados</h1>

<%= form_for(:session, :html => {class: "form-horizontal", role: "form"}, url: find_dup_path) do |f|  %>
  <div class="form-group">
    <div class="control-label col-sm-2">
      <%= f.label "API Key"  %>
    </div> 
    <div class="col-sm-4">
      <%= f.text_field :apikey, class: "form-control", placeholder: "Introducir el API KEY", autofocus: true %>
    </div>
  </div>
  <div class="form-group">
    <div class="control-label col-sm-2">
      <%= f.label "Frase"  %> <br/>
    </div>
    <div class="col-sm-8">
      <%= f.text_field :sentence, class: "form-control", placeholder: "Introducir la frase que deseas validar" %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">  
       <!-- si es un nuevo registro coloca el primer string, sino, el segundo -->
      <%= f.submit "Buscar repetidos", class: 'btn btn-primary btn-lg'%> 
    </div>
  </div>
<% end %>
<div class="col-xs-5 col-xs-offset-5">
  [<%= link_to "Regresar a la lista de APIS" , root_path, class: "link" %>]
</div>