<H1 align = 'center'> Listado de APIS</H1>

<div class="container">
  <table class="table table-striped table table-bordered">
    <thead>
      <tr>
        <th>Email</th>
        <th>Key</th>
        <th>Numero de request</th>
      </tr>
    </thead>
    <tbody>
      <% @apis.each do |api| %>
        <tr>
          <td> <%= api.email %></td>
          <td><%= api.key %></td>
          <td><%= api.number_request %></td>
        </tr>
      <%end %>
    </tbody>
  </table>
</div>
<div class="col-xs-5 col-xs-offset-5">
  <%= link_to 'Crear API', new_api_path %> | 
  <%= link_to 'Conseguir duplicados', find_dup_path %>
</div>
